        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".HomeMainActivity" />
        <activity android:name=".CameraPanic" />
        <activity android:name=".happenings.MiHappenings" />
        <activity android:name=".notification.MiNotification" />
        <activity android:name=".panic.MiPanic" />
        <activity android:name=".setting.MiSetting"
            android:theme="@style/MyAppTheme"/>
        <activity android:name=".circle.CircleActivity" />
        <activity android:name=".location.LocationActivity" />
        <activity android:name=".friend.FriendActivity" />
        <activity android:name=".setting.FeedBackActivity" />
        <activity android:name=".setting.FAQSActivity" />
        <activity android:name=".circle.Create_Private_Circle" />
        <activity android:name=".circle.Create_Public_Circle" />
        <activity android:name=".circle.Final_circle_Creation" />
        <activity android:name=".circle.Circle_Landing" />


    </application>

</manifest>